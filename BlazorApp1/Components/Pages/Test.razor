@page "/test"

<h3>Test Input</h3>

<input type="text" @oninput="HandleInput" placeholder="Type here..." />

<button @onclick="TestButtonClick">Test Button</button>

@code {
    private async Task HandleInput(ChangeEventArgs e)
    {
        var inputValue = e.Value?.ToString() ?? string.Empty;
        Console.WriteLine($"Input Value: {inputValue}"); // Log the input value
    }

    private void TestButtonClick()
    {
        Console.WriteLine("Button Clicked"); // Log button click
    }
}